@model List<Tusimaka.Models.Kunde>

@{
    ViewBag.Title = "Bekreftelse";
}

<h2>Bekreftelse</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th></th>
    </tr>


@foreach (var item in Model) {
    <tr>
        <td>
            @item.Fornavn
        </td>
    </tr>
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.KundeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.KundeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.KundeID })
        </td>
    </tr>
}

</table>
