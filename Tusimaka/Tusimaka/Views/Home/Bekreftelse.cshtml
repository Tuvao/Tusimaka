@model List<Tusimaka.Models.KomplettReise>

@{
    ViewBag.Title = "Bekreftelse";
}

<h2>Bekreftelse</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KundeID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FlyBestillingsID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BetalingsID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KundeID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FlyBestillingsID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BetalingsID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
